public class BoxCreation{
     public static void main(String []args){
         int i,j,n=10;
         for(i=0;i<=n;i++){
             for(j=0;j<=n;j++){
                if(j==0 || j==n || i==0 || i==n)
                System.out.print("$");
                else
                System.out.print(" ");
             }
             System.out.println();
         }
     }
}
